#!/usr/bin/env ruby

$LIB_DIR = File.join(File.dirname(__FILE__), "..", "lib")
$LOAD_PATH << $LIB_DIR 

require "collective"
require "command"

load "network"

#command = Command.new
#command.run(*ARGV)

@hypervisor = Hypervisor.new

@subnet.guests.each do |guest|
  pp guest
  if guest.enabled
    @hypervisor.create(guest)
    @hypervisor.start(guest)
  end
end
